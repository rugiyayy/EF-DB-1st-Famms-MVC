

@{
    ViewData["Title"] = "Products";
    ViewData["InnerPageSection"] = "Product Grid";
}



<!-- product section -->
<section class="product_section layout_padding">
    <div class="container">
        <div class="heading_container heading_center">
            <h2>
                Our <span>products</span>
            </h2>
        </div>
        <div class="row">
            @foreach (var product in Model.ProductsTbls)
            {
                <partial name="_ProductPartialView" model="product" />
            }




        </div>
        <div class="btn-box">
            <a href="">
                View All products
            </a>
        </div>
    </div>
</section>
<!-- end product section -->
